&ecspi3 {
	#address-cells = <1>;
	#size-cells = <0>;

	status = "okay";

	spidev0: spidev@0{
		compatible = "linux,spidev";
		reg = <0>;      /* CE0 */
		#address-cells = <1>;
		#size-cells = <0>;
		spi-max-frequency = <500000>;
	};
};
