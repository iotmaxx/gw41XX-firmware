image @IMAGE@ {
	rauc {
		file root.ext4 { image = "root.ext2" }
		manifest = "
			[update]
			compatible=@RAUC_BUNDLE_COMPATIBLE@
			version=@RAUC_BUNDLE_VERSION@
			build=@RAUC_BUNDLE_BUILD@
			description=@RAUC_BUNDLE_DESCRIPTION@

			[image.rootfs]
			filename=root.ext4
			"
		cert = "@RAUC_CERT@"
		key = "@RAUC_KEY@"
		keyring = "@RAUC_KEYRING@"
	}
}
