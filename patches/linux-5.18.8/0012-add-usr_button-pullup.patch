+			MX7D_PAD_EPDC_DATA05__GPIO2_IO5			0x14 /* SIM_CC2 */
 		>;
 	};
 
@@ -510,6 +510,8 @@ pinctrl_gpio3: gpio3grp {
 		fsl,pins = <
 			MX7D_PAD_LCD_RESET__GPIO3_IO4			0x79 /* VUSB_GSM_EN */
 			MX7D_PAD_LCD_DATA20__GPIO3_IO25			0x14 /* SIM_ENABLE */
