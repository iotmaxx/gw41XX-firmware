@@ -248,7 +248,7 @@ &gpio2 {
 			  "", "", "", "", "gsm_pwr_en_3v3",
 			  "", "", "n_usbhub_int", "usbhub_connect", "",
 			  "", "", "", "", "",
-			  "", "", "", "", "n_usbhub_rst",
+			  "", "", "", "n_rtc_int_a", "n_usbhub_rst",
 			  "";
 };
 
