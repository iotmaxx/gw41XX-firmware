+			  "n_pmic_int", "n_usr_button", "", "", "",
 			  "";
 };
 
@@ -502,7 +502,7 @@ pinctrl_gpio2: gpio2grp {
 		fsl,pins = <
 			MX7D_PAD_EPDC_DATA15__GPIO2_IO15		0x14 /* GSM_PWR_EN_3V3 */
 			MX7D_PAD_EPDC_DATA04__GPIO2_IO4			0x14 /* SIM_CD1 */
