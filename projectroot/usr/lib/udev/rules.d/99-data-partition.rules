IMPORT{builtin}="path_id"
IMPORT{cmdline}="bootchooser.active"
IMPORT{cmdline}="root"

ENV{bootchooser.active}=="system0", GOTO="data_partitions_system_a"
ENV{bootchooser.active}=="system1", GOTO="data_partitions_system_b"
ENV{root}=="/dev/nfs", GOTO="data_partitions_system_a"
GOTO="data_partitions_end"

LABEL="data_partitions_system_a"
ENV{ID_PATH}=="platform-30b60000.usdhc", ENV{PARTN}=="9", SYMLINK+="data", SYMLINK+="data1"
ENV{ID_PATH}=="platform-30b60000.usdhc", ENV{PARTN}=="10", SYMLINK+="data_inactive", SYMLINK+="data2"
GOTO="data_partitions_end"

LABEL="data_partitions_system_b"
ENV{ID_PATH}=="platform-30b60000.usdhc", ENV{PARTN}=="10", SYMLINK+="data", SYMLINK+="data2"
ENV{ID_PATH}=="platform-30b60000.usdhc", ENV{PARTN}=="9", SYMLINK+="data_inactive", SYMLINK+="data1"

LABEL="data_partitions_end"
